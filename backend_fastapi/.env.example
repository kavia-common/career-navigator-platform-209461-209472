# Backend FastAPI - .env.example
# Copy this file to .env and adjust values for your environment.

# SQLite or other SQLAlchemy URL. Use absolute path for Docker volume mount.
DATABASE_URL=sqlite:///./career_nav.db
# In Docker Compose this should be http://llm:8001/recommend
LLM_SERVICE_URL=http://localhost:8001/recommend
# Comma-separated list of allowed origins or * for all (MVP)
CORS_ORIGINS=*
# Uvicorn configuration (optional)
UVICORN_HOST=0.0.0.0
UVICORN_PORT=8000
